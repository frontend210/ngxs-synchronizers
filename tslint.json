{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "only-arrow-functions": false,
        "max-line-length": false,
        "no-namespace": false,
        "trailing-comma": false,
        "max-classes-per-file": false,
        "arrow-parens": false,
        "no-console": false,
        "interface-over-type-literal": false,
        "array-type": false,
        "interface-name": false,
        "variable-name": false,
        "forin": false
    },
    "rulesDirectory": []
}